<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 180 180" width="180" height="180">
  <!-- Background with rounded corners for iOS -->
  <rect x="0" y="0" width="180" height="180" fill="#4f46e5" rx="20"/>
  
  <!-- Book pages -->
  <rect x="45" y="65" width="45" height="60" fill="#ffffff" rx="3"/>
  <rect x="90" y="65" width="45" height="60" fill="#f3f4f6" rx="3"/>
  
  <!-- Book spine -->
  <rect x="87" y="65" width="6" height="60" fill="#6366f1"/>
  
  <!-- Book lines (text) -->
  <line x1="55" y1="80" x2="80" y2="80" stroke="#6b7280" stroke-width="2"/>
  <line x1="55" y1="90" x2="75" y2="90" stroke="#6b7280" stroke-width="2"/>
  <line x1="55" y1="100" x2="80" y2="100" stroke="#6b7280" stroke-width="2"/>
  <line x1="55" y1="110" x2="70" y2="110" stroke="#6b7280" stroke-width="2"/>
  
  <line x1="100" y1="80" x2="125" y2="80" stroke="#9ca3af" stroke-width="2"/>
  <line x1="100" y1="90" x2="120" y2="90" stroke="#9ca3af" stroke-width="2"/>
  <line x1="100" y1="100" x2="125" y2="100" stroke="#9ca3af" stroke-width="2"/>
  <line x1="100" y1="110" x2="115" y2="110" stroke="#9ca3af" stroke-width="2"/>
  
  <!-- AI/Brain symbol -->
  <circle cx="65" cy="45" r="15" fill="#fbbf24" stroke="#f59e0b" stroke-width="2"/>
  <circle cx="60" cy="40" r="3" fill="#f59e0b"/>
  <circle cx="70" cy="40" r="3" fill="#f59e0b"/>
  <circle cx="65" cy="50" r="2" fill="#f59e0b"/>
  
  <!-- Neural network lines -->
  <line x1="55" y1="50" x2="45" y2="60" stroke="#fbbf24" stroke-width="2"/>
  <line x1="75" y1="50" x2="85" y2="60" stroke="#fbbf24" stroke-width="2"/>
  <line x1="65" y1="60" x2="65" y2="70" stroke="#fbbf24" stroke-width="2"/>
  
  <!-- Additional AI connections -->
  <circle cx="45" cy="60" r="2" fill="#fbbf24"/>
  <circle cx="85" cy="60" r="2" fill="#fbbf24"/>
</svg>
